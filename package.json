{
  "name": "askql-front",
  "version": "0.0.1",
  "description": "AskQL Electron Application",
  "main": "main.js",
  "author": "AskQL Team",
  "license": "ISC",
  "homepage": "https://github.com/AskQL/Front#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AskQL/Front.git"
  },
  "bugs": {
    "url": "https://github.com/AskQL/Front/issues"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish never"
  },
  "devDependencies": {
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.askql.askql-app",
    "productName": "AskQL",
    "files": [
      "main.js",
      "package.json",
      "build/",
      "dist/"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.icns",
      "target": "dmg"
    },
    "win": {
      "icon": "build/icon.ico",
      "target": "nsis"
    }
  }
}
